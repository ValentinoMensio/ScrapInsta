{
  "manifest_version": 3,
  "name": "ScrapInsta Enqueuer",
  "description": "Encola fetch_followings en tu servidor, usando la cuenta del cliente (X-Account).",
  "version": "0.1.0",
  "action": {
    "default_title": "Encolar followings",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}